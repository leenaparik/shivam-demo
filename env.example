# Copy this file to ".env" (same folder as docker-compose.yml), then edit values.
#
# Generate a Fernet key for SSN encryption:
#   python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"

MYSQL_ROOT_PASSWORD=rootpass
MYSQL_DATABASE=appdb
MYSQL_USER=appuser
MYSQL_PASSWORD=apppass

# Flask session secret (change this in real usage)
FLASK_SECRET_KEY=dev-secret-change-me

# The UI's browser origin (used for CORS)
UI_ORIGIN=http://localhost:8080

# REQUIRED: Fernet key used to encrypt SSN at rest in the DB
SSN_KEY=


